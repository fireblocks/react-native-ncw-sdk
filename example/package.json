{
  "name": "@fireblocks/react-native-ncw-sdk-example",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "android": "react-native run-android",
    "ios": "react-native run-ios",
    "start": "react-native start",
    "build:android": "cd android && ./gradlew assembleDebug --no-daemon --console=plain -PreactNativeArchitectures=arm64-v8a",
    "build:ios": "cd ios && xcodebuild -workspace ReactNativeNcwSdkExample.xcworkspace -scheme ReactNativeNcwSdkExample -configuration Debug -sdk iphonesimulator CC=clang CPLUSPLUS=clang++ LD=clang LDPLUSPLUS=clang++ GCC_OPTIMIZATION_LEVEL=0 GCC_PRECOMPILE_PREFIX_HEADER=YES ASSETCATALOG_COMPILER_OPTIMIZATION=time DEBUG_INFORMATION_FORMAT=dwarf COMPILER_INDEX_STORE_ENABLE=NO"
  },
  "dependencies": {
    "@react-native-clipboard/clipboard": "^1.13.2",
    "@react-native-firebase/app": "^18.8.0",
    "@react-native-firebase/auth": "^18.8.0",
    "@react-native-google-signin/google-signin": "^10.1.2",
    "@robinbobin/react-native-google-drive-api-wrapper": "^1.2.4",
    "js-base64": "^3.7.6",
    "react": "18.2.0",
    "react-native": "0.73.6",
    "react-native-autocomplete-dropdown": "^3.1.4",
    "react-native-dropdown-picker": "^5.4.6",
    "react-native-get-random-values": "^1.11.0",
    "react-native-keychain": "^8.2.0",
    "react-native-mmkv": "^2.12.1",
    "react-native-progress": "^5.0.1",
    "react-native-reanimated-table": "^0.0.2",
    "react-native-securerandom": "^1.0.1",
    "react-native-svg": "^15.0.0",
    "react-native-uuid": "^2.0.1",
    "react-native-vision-camera": "^3.9.2",
    "react-qr-code": "^2.0.12",
    "rn-secure-storage": "^3.0.1",
    "socket.io-client": "^4.7.4",
    "zustand": "^4.5.0"
  },
  "devDependencies": {
    "@babel/core": "^7.20.0",
    "@babel/preset-env": "^7.20.0",
    "@babel/runtime": "^7.20.0",
    "@react-native/babel-preset": "0.73.21",
    "@react-native/metro-config": "0.73.5",
    "@react-native/typescript-config": "0.73.1",
    "babel-plugin-module-resolver": "^5.0.0"
  },
  "engines": {
    "node": ">=18"
  }
}
